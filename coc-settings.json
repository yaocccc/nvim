    "explorer.keyMappings.global.l": ["expandable?", "expand", "open"],
    "explorer.keyMappings.global.J": ["toggleSelection", "nodeNext"],
    "explorer.keyMappings.global.K": ["toggleSelection", "nodePrev"],
    "explorer.keyMappings.global.o": ["expandable?", "cd", "open"],
    "explorer.keyMappings.global.<bs>": "gotoParent",
    "explorer.keyMappings.global.q": "quit",
    "explorer.keyMappings.global.Q": "quit",
    "explorer.keyMappings.global.<c-c>": "esc",
    "explorer.keyMappings.global.d": "delete",
    "explorer.keyMappings.global.D": "deleteForever",
    "explorer.keyMappings.global.c": "copyFile",
    "explorer.keyMappings.global.C": "cutFile",
    "explorer.keyMappings.global.y": "copyFilename",
    "explorer.keyMappings.global.Y": "copyFilepath",
    "explorer.keyMappings.global.<": ["wait", "markPrev:modified"],
    "explorer.keyMappings.global.>": ["wait", "markNext:modified"],
    "explorer.keyMappings.global.(": ["wait", "markPrev:git"],
    "explorer.keyMappings.global.)": ["wait", "markNext:git"],
    "markdownlint.config": {
        "default": true,
        "line_length": false,
        "MD004": false /* 允许无序号风格     */,
        "MD009": false /* 允许不定长的空格   */,
        "MD012": false /* 允许连续的空行     */,
        "MD022": false /* 标题前后允许非空   */,
        "MD031": false /* 允许代码块前后非空 */,
        "MD032": false /* 允许列表前后非空   */,
        "MD033": false /* 允许使用html语法   */,
        "MD041": false /* 首行允许为非H1     */,
        "MD042": false /* 首行空链接         */,
        "MD046": false /* unknown            */
    },
    "translator.engines": ["youdao"],
    "tsserver.trace.server": "messages",
    "tsserver.enableJavascript": true,
    "tsserver.enable": false,
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterDelete",
    "git.changedSign.text": "┃",
    "git.addedSign.text": "┃",
