{
    "snippets.userSnippetsDirectory": "$NVIM/snippets",
    "tabnine.priority": 90,
    "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
    "pairs.enableBackspace": true,
    "markdownlint.config.default": true,
    "markdownlint.config.line_length": false,
    "translator.engines": ["google", "youdao", "haici"],
    "tsserver.trace.server": "messages",
    "tsserver.enableJavascript": true,
    "explorer.quitOnOpen": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.floating.border.title": "",
    "explorer.openAction.strategy": "sourceWindow",
    "explorer.presets.floating.position": "floating",
    "explorer.presets.floating.floating-width": 80,
    "explorer.presets.floating.floating-position": "center",
    "explorer.file.column.git.icon.mixed": "*",
    "explorer.file.column.git.icon.modified": "~",
    "explorer.file.column.git.icon.added": "+",
    "explorer.file.column.git.icon.deleted": "-",
    "explorer.file.column.git.icon.renamed": "+",
    "explorer.file.column.git.icon.copied": "+",
    "explorer.file.column.git.icon.unmerged": "!",
    "explorer.file.column.git.icon.untracked": "?",
    "explorer.file.column.git.icon.ignored": "#",
    "explorer.keyMappings.i": false,
    "explorer.keyMappings.<tab>": "toggleSelection",
    "explorer.keyMappings.<2-LeftMouse>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "explorer.keyMappings.<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "explorer.keyMappings.<right>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "explorer.keyMappings.<left>": "collapse",
    "explorer.keyMappings.h": "collapse",
    "explorer.keyMappings.l": ["expandable?", "expand", "open"],
    "explorer.keyMappings.J": ["toggleSelection", "nodeNext"],
    "explorer.keyMappings.K": ["toggleSelection", "nodePrev"],
    "explorer.keyMappings.o": ["expandable?", "cd", "open"],
    "explorer.keyMappings.<bs>": "gotoParent",
    "explorer.keyMappings.q": "quit",
    "explorer.keyMappings.Q": "quit",
    "explorer.keyMappings.<c-c>": "esc",
    "explorer.keyMappings.<<": "gitStage",
    "explorer.keyMappings.>>": "gitUnstage",
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterDelete",
    "git.branchCharacter": "",
    "git.showCommitInFloating": true
}
