    "java.configuration.runtimes": [
        {
            "name": "JavaSE-14",
            "path": "/opt/jdk-14.0.2",
            "default": true
        }
    ],
    "clangd.path": "/home/chenyc/.config/coc/extensions/coc-clangd-data/install/11.0.0/clangd_11.0.0/bin/clangd",
